           if glass_only$ = "Y" then ct_descr$ = "GLON      "
           if grid_only$  = "Y" then ct_descr$ = "GDON      "

           if  str(ct_grid$,2%,1%) = "Z" then ct_descr$ = "SPGD      "

           if ct_wind_prc < .01 and scr_sel$ <> "9" then ct_descr$ = "NOCG      "

           str(ct_rec$,184%,1%)  = ct_wood$
           str(ct_rec$,185%,10%) = ct_descr$
           str(ct_rec$,195%,25%) = ct_part$
                                            /* (PAR000) - Special Mod       */
                                            /* (RHHTEST) - Special Test     */
        REM   grid_only$  = "Y"             /* Grid Only                    */ 

           glass_only$ = "Y"                /*  For Glass Only              */


                                            /* (RHHTEST) - Special Test     */ 
        return

        convert_to_exact                      /* In Decimal       */  
           size1 = 0.0                        /* xxxx.xxxx        */
           a$ = str(size$,1%,3%)              /* Whole Part       */ 
           b$ = str(size$,5%,5%)              /* Fraction Part    */
           for kk% = 1% to 100% step 5%
               if str(sz$,kk%,5%) = b$ then goto C1
           next kk%
           if kk% > 76% then kk% = 76%        /* Set to .0000     */
C1:
           c$ = a$ & str(sz1$,kk%,5%)

           convert c$ to size1, data goto C1A
C1A:
           init(" ") size$
           convert size1 to size$, pic(####.####)

        return

        lookup_glass                           /* Cust Glass Flags     */
           init(" ") readkey$, descr$, ct_gl_type$
           ct_gl_type$ = "NNNNNN"

           str(readkey$, 1%,9%)  = "CUSTGLASS"
           str(readkey$,10%,15%) = ct_glass$
           read #1,key = readkey$, using LOOKUP_1, descr$,               ~
                                       eod goto LOOKUP_2
LOOKUP_1:       FMT POS(25), CH(30)

           ct_gl_type$ = str(descr$,1%,6%)
LOOKUP_2:
        return

        lookup_lites                           /* Cust Glass Flags     */
           init(" ") readkey$, descr$, ct_no_lites$
           ct_no_lites$ = "00"
           ct_no_lites% = 0%

           str(readkey$, 1%,9%)  = "CUSTLITES"
           str(readkey$,10%,15%) = ct_grid$
           read #1,key = readkey$, using LOOKUP_1, descr$,               ~
                                       eod goto LOOKUP_3
           ct_no_lites$ = str(descr$,1%,2%)
           convert ct_no_lites$ to ct_no_lites%, data goto LOOKUP_3

LOOKUP_3:
        return

        lookup_window_price                    /* (APCPLNDT) Find Price */
           ct_wind_prc = 0.1                   /* Prevent Division by Zero */

           if scr_sel$ = "9" then return       /* Run from Report       */

           init(" ") dt_key4$, ct_wind_prc$, ct_part$, ct_cross$

           dt_key4$ = str(ct_rec$,12%,8%)      /* Warranty Id.          */
           read #3,key 4% = dt_key4$, using LOOKUP_4, ct_wind_prc, ct_part$, ~
                                              ct_special$,eod goto LOOKUP_5
LOOKUP_4:     FMT POS(133), PD(14,4), POS(189), CH(25), POS(220), CH(10)

LOOKUP_5:
           convert ct_wind_prc to ct_wind_prc$, pic(####.####-)

           ct_cross$ = str(ct_part$,7%,1%) & str(ct_part$,10%,1%)

        return     

        lookup_grid_subscript                 /* Find Talble Subscript*/
           init(" ") readkey$, descr$, k$
           k$ = "00"

           str(readkey$, 1%,9%)  = "CUSTGRID "
           str(readkey$,10%,15%) = ct_grid$
           read #1,key = readkey$, using LOOKUP_1, descr$,               ~
                                       eod goto LOOKUP_6
           k$ = str(descr$,1%,2%)
LOOKUP_6:
        return

                                 
        debug_display_screen
            if debug% = 0% then return             /* Debug Turned Off */

